    SQL.Strings = (
      'SELECT B.BANDGRID_FREQ_BEGIN, '
      '            B.BANDGRID_FREQ_END,'
      '           M.MODE_BACKGROUND_COLOR'
      'FROM  '
      'BANDGRID B, MODES  M WHERE '
      'B.BANDGRID_MODE_NAME=M.MODE_NAME'
      'AND  B.BANDGRID_BAND_MHZ=:BANDGRID_BAND_MHZ'
      'ORDER BY BANDGRID_FREQ_BEGIN,BANDGRID_FREQ_END')
